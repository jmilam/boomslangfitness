I"Õ(function() {
  $('.likes i, .dislikes i').on('click', function() {
    $.ajax({
      dataType: 'script',
      type: 'POST',
      url: '/workout/rate_workout',
      data: {
        id: $('#workout').val(),
        type: $(this).data('type'),
        user_id: $('#user').val()
      },
      success: function(data) {
        $('.likes i').text(JSON.parse(data).likes);
        $('.dislikes i').text(JSON.parse(data).dislikes);
      }
    });
  });

}).call(this);
